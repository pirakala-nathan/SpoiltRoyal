= form_for @service_location do |f|
  - if @service_location.errors.any?
    #error_explanation
      %h2= "#{pluralize(@service_location.errors.count, "error")} prohibited this service_location from being saved:"
      %ul
        - @service_location.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :vendor_id
    = f.number_field :vendor_id
  .field
    = f.label :city_id
    = f.number_field :city_id
  .actions
    = f.submit 'Save'
